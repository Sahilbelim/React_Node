doctype html
html(lang='en')
  head
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    title Document
    link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css', rel='stylesheet', integrity='sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9', crossorigin='anonymous')
  body
    nav.navbar.navbar-expand-lg.bg-body-tertiary
      .container-fluid
        a.navbar-brand(href='#') Navbar
        button.navbar-toggler(type='button', data-bs-toggle='collapse', data-bs-target='#navbarSupportedContent', aria-controls='navbarSupportedContent', aria-expanded='false', aria-label='Toggle navigation')
          span.navbar-toggler-icon
        #navbarSupportedContent.collapse.navbar-collapse
          ul.navbar-nav.justify-content-center
            li.nav-item.p-3
              a.nav-link(href='#')  home
            li.nav-item.p-3
              a.nav-link(href='#') About us
            li.nav-item.p-3
              a.nav-link(href='#') Contect us
            li.nav-item.p-3
              a.nav-link(href='#') Shop 
            li.nav-item.p-3
              a.nav-link(href='#') Services 
            li.nav-item.p-3
              .btn.btn-primary
                a.nav-link(href='#') Log in 
    #carouselExample.carousel.slide
      .carousel-inner
        .carousel-item.active
          img.d-block.w-100(src='https://picsum.photos/1900/700?1', alt='...')
        .carousel-item
          img.d-block.w-100(src='https://picsum.photos/1900/700?2', alt='...')
        .carousel-item
          img.d-block.w-100(src='https://picsum.photos/1900/700?3', alt='...')
      button.carousel-control-prev(type='button', data-bs-target='#carouselExample', data-bs-slide='prev')
        span.carousel-control-prev-icon(aria-hidden='true')
        span.visually-hidden Previous
      button.carousel-control-next(type='button', data-bs-target='#carouselExample', data-bs-slide='next')
        span.carousel-control-next-icon(aria-hidden='true')
        span.visually-hidden Next
    h1.mt-5(align='center')  shop 
    .container.mt-5
      .row
       -var count=0
        each data in all_data 
            
              .col-12.col-md-4.col-lg-3.col-sm-6.mt-5
                .card(style='width: 18rem;')
                    img.card-img-top(src=`https://picsum.photos/300?${data.productCode}`, alt='img not found')
                    .card-body
                    h5.card-title.p-3  Name : #{data.productName}
                        
                    pre.p-3 price : #{data.buyPrice}
                    a.m-4.btn.btn-primary(href='#') buy now
    script(src='https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js', integrity='sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm', crossorigin='anonymous')